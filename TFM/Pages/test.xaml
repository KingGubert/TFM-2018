                                            </DataTrigger>


                                            <!--Heat Placement Bonus-->
                                            <DataTrigger Binding="{Binding PB_2}" Value="6">
                                                <Setter Property="Fill" >
                                                    <Setter.Value>
                                                        <ImageBrush ImageSource="..\Res\Icons\Heat25x25.png"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>


                                        </Style.Triggers>
                                    </Style>
                                </Rectangle.Style>
                            </Rectangle>

                            <Rectangle Width="11" Height="11" Canvas.Left="32.5" Canvas.Top="15" Stretch="Fill" StrokeLineJoin="Round" >
                                <Rectangle.Style>
                                    <Style TargetType="Rectangle">
                                        <Style.Setters>
