                            <Rectangle Width="11" Height="11" Canvas.Left="47.5" Canvas.Top="20" Stretch="Fill" StrokeLineJoin="Round" >
                                <Rectangle.Style>
                                    <Style TargetType="Rectangle">
                                        <Style.Setters>
                                            <Setter Property="Fill" Value="Transparent"/>
                                            <Setter Property="Stroke" Value="Transparent"/>
                                        </Style.Setters>
