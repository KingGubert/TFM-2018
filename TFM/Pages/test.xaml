                                <TextBlock.RenderTransform>
                                    <TransformGroup>
                                        <RotateTransform Angle="30"/>
                                    </TransformGroup>
                                </TextBlock.RenderTransform>-->
		                        <TextBlock TextAlignment="Center" FontSize="12" FontWeight="Bold" Width="60" Canvas.Left="8" Canvas.Top="25" TextWrapping="Wrap" Text="{Binding SpotName}" Foreground="#FF000000"/>
                            <!--</TextBlock>-->

                            <Button Canvas.Left="5" Canvas.Top="4">
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}">
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Button}">
                                                    <Border>
                                                        <Path x:Name="thisbutton" Width="65.7501" Height="67.9996" Stretch="Fill" StrokeLineJoin="Round" Stroke="#80000000" Data="F1 M 938.073,197.902L 938.073,238.102L 970.448,251.502L 1002.82,238.102L 1002.82,197.902L 970.448,184.502L 938.073,197.902 Z ">
                                                            <Path.Style>
                                                                <Style TargetType="Path">
                                                                    <Setter Property="Fill" Value="Transparent"/>
                                                                    <Setter Property="Opacity" Value="1"></Setter>

                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding SpotType}" Value="1">
                                                                            <Setter Property="Fill" Value="DarkBlue"></Setter>
                                                                            <Setter Property="Opacity" Value="0.5"></Setter>

                                                                        </DataTrigger>



                                                                    </Style.Triggers>





                                                                </Style>

                                                            </Path.Style>

                                                            <Path.Effect>
                                                                <DropShadowEffect BlurRadius="8" ShadowDepth="7.55906" Opacity="1" Color="#FF000000" Direction="315"/>
                                                            </Path.Effect>
                                                        </Path>
                                                    </Border>
                                                    <ControlTemplate.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Fill" TargetName="thisbutton" Value="#FFBEE6FD"/>
                                                        </Trigger>
                                                        <Trigger Property="IsPressed" Value="True">
                                                            <Setter Property="Fill" TargetName="thisbutton" Value="#FFC4E5F6"/>
                                                        </Trigger>
                                                    </ControlTemplate.Triggers>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>

                                    </Style>
                                </Button.Style>
                            </Button>

                            <Rectangle Width="11" Height="11" Canvas.Left="17.5" Canvas.Top="20" Stretch="Fill" StrokeLineJoin="Round">
                                <Rectangle.Style>
                                    <Style TargetType="Rectangle">
                                        <Style.Setters>
                                            <Setter Property="Fill" Value="Transparent"/>
                                            <Setter Property="Stroke" Value="Transparent"/>
                                        </Style.Setters>
                                        <Style.Triggers>

                                            <!--Steel Placement Bonus-->
                                            <DataTrigger Binding="{Binding PB_2}" Value="1">
                                                <Setter Property="Fill" >
                                                    <Setter.Value>
                                                        <ImageBrush ImageSource="..\Res\Icons\Steel100x100.png"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>

                                            <!--Titan Placement Bonus-->
                                            <DataTrigger Binding="{Binding PB_2}" Value="2">
                                                <Setter Property="Fill" >
                                                    <Setter.Value>
                                                        <ImageBrush ImageSource="..\Res\Icons\Titan100x100.png"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>

                                            <!--Plant Placement Bonus-->
                                            <DataTrigger Binding="{Binding PB_2}" Value="3">
                                                <Setter Property="Fill" >
                                                    <Setter.Value>
                                                        <ImageBrush ImageSource="..\Res\Icons\Plant100x100.png"/>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>

                                            <!--Card Placement Bonus-->
                                            <DataTrigger Binding="{Binding PB_2}" Value="4">
                                                <Setter Property="Fill" >
                                                    <Setter.Value>
